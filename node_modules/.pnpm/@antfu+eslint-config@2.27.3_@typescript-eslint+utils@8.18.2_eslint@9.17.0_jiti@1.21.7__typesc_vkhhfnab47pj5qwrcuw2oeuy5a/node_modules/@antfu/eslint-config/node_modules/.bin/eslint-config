else
  export NODE_PATH="/Users/zhouhua/work/waveform-player/node_modules/.pnpm/@antfu+eslint-config@2.27.3_@typescript-eslint+utils@8.18.2_eslint@9.17.0_jiti@1.21.7__typesc_vkhhfnab47pj5qwrcuw2oeuy5a/node_modules/@antfu/eslint-config/bin/node_modules:/Users/zhouhua/work/waveform-player/node_modules/.pnpm/@antfu+eslint-config@2.27.3_@typescript-eslint+utils@8.18.2_eslint@9.17.0_jiti@1.21.7__typesc_vkhhfnab47pj5qwrcuw2oeuy5a/node_modules/@antfu/eslint-config/node_modules:/Users/zhouhua/work/waveform-player/node_modules/.pnpm/@antfu+eslint-config@2.27.3_@typescript-eslint+utils@8.18.2_eslint@9.17.0_jiti@1.21.7__typesc_vkhhfnab47pj5qwrcuw2oeuy5a/node_modules/@antfu/node_modules:/Users/zhouhua/work/waveform-player/node_modules/.pnpm/@antfu+eslint-config@2.27.3_@typescript-eslint+utils@8.18.2_eslint@9.17.0_jiti@1.21.7__typesc_vkhhfnab47pj5qwrcuw2oeuy5a/node_modules:/Users/zhouhua/work/waveform-player/node_modules/.pnpm/node_modules:$NODE_PATH"
